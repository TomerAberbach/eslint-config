{
  "name": "@tomer/eslint-config",
  "version": "0.0.11",
  "author": "Tomer Aberbach <tomeraberbach@gmail.com> (https://tomeraberba.ch)",
  "description": "My ESLint configuration!",
  "keywords": [
    "eslint",
    "eslintconfig",
    "eslint-config",
    "lint",
    "configuration",
    "config"
  ],
  "homepage": "https://github.com/TomerAberbach/eslint-config",
  "repository": "TomerAberbach/eslint-config",
  "bugs": {
    "url": "https://github.com/TomerAberbach/eslint-config/issues"
  },
  "license": "MIT",
  "files": [
    "index.js"
  ],
  "main": "index.js",
  "scripts": {
    "lint:prettier:base": "prettier --loglevel silent",
    "lint:prettier": "pnpm run lint:prettier:base -- --write .",
    "lint:eslint:base": "eslint -c ./index.js --cache --ext mjs,cjs,js --ignore-path .gitignore",
    "lint:eslint": "pnpm run lint:eslint:base -- --fix .",
    "lint": "run-s lint:*",
    "prepublishOnly": "pnpm run lint"
  },
  "prettier": "@tomer/prettier-config",
  "dependencies": {
    "eslint-plugin-ava": "11.0.0",
    "eslint-plugin-compat": "3.9.0",
    "eslint-plugin-jsx-a11y": "6.4.1",
    "eslint-plugin-react": "7.21.5",
    "eslint-plugin-sort-imports-es6-autofix": "0.5.0"
  },
  "peerDependencies": {
    "babel-eslint": "^10.0.0",
    "eslint": "^7.0.0"
  },
  "devDependencies": {
    "@tomer/prettier-config": "0.0.2",
    "babel-eslint": "10.1.0",
    "eslint": "7.16.0",
    "npm-run-all": "4.1.5",
    "prettier": "2.2.1"
  }
}
